{
	"name": "nextjs-capacitor-materialui",
	"version": "1.0.0",
	"private": true,
	"engines": {
		"node": ">=16.17.0",
		"npm": ">=8.15.0",
		"yarn": "please-use-npm"
	},
	"scripts": {
		"dev": " cross-env NODE_OPTIONS='--inspect' next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\"",
		"prettier": "prettier --write .",
		"lint:fix": "eslint src --fix && npm run format",
		"lint:strict": "eslint --max-warnings=0 --ext js,tsx,mdx,md src ",
		"typecheck": "tsc --noEmit --incremental false",
		"test:watch": "jest --watch",
		"test": "jest",
		"format": "prettier -w .",
		"format:check": "prettier -c .",
		"dev:up": "docker-compose -f docker-compose.dev.yml up  --build --force-recreate",
		"prod:up": "docker-compose up --build --force-recreate",
		"static": "next build && next export",
		"android": "next build && next export && cap copy android && cap run android",
		"ios": "next build && next export && cap copy ios && cap run ios",
		"build:android": "next build && next export && cap copy android && cd android && ./gradlew bundleRelease",
		"build:ios": "next build && next export && cap copy ios && xcodebuild -project ./ios/App/App.xcodeproj -allowProvisioningUpdates",
		"copy:ios": "cap copy ios",
		"copy:android": "cap copy android",
		"sync:ios": "cap sync ios",
		"sync:android": "cap sync android"
	},
	"dependencies": {
		"@capacitor/android": "^5.3.0",
		"@capacitor/app": "^5.0.6",
		"@capacitor/cli": "^5.3.0",
		"@capacitor/core": "^5.3.0",
		"@capacitor/ios": "^5.3.0",
		"@emotion/cache": "^11.11.0",
		"@emotion/react": "^11.11.1",
		"@emotion/server": "^11.11.0",
		"@emotion/styled": "^11.11.0",
		"@fontsource/roboto": "^5.0.8",
		"@lukemorales/query-key-factory": "^1.3.2",
		"@mui/icons-material": "^5.14.8",
		"@mui/material": "^5.14.8",
		"@mui/styled-engine-sc": "^5.14.8",
		"@tanstack/react-query": "^4.35.0",
		"@tanstack/react-virtual": "^3.0.0-alpha.0",
		"axios": "^0.27.2",
		"clsx": "^1.2.1",
		"date-fns": "^2.29.2",
		"dayjs": "^1.11.9",
		"framer-motion": "^6.5.1",
		"html-react-parser": "^4.2.2",
		"next": "^12.2.5",
		"next-contentlayer": "^0.2.7",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-icons": "^4.4.0",
		"react-material-ui-carousel": "^3.4.2",
		"styled-components": "^5.3.11",
		"zod": "^3.22.2"
	},
	"devDependencies": {
		"@babel/core": "^7.18.13",
		"@commitlint/cli": "^17.1.2",
		"@commitlint/config-conventional": "^17.1.0",
		"@mdx-js/react": "^2.1.3",
		"@types/node": "^18.7.14",
		"@types/react": "^18.0.18",
		"@types/react-dom": "18.0.6",
		"@typescript-eslint/eslint-plugin": "^5.36.1",
		"@typescript-eslint/parser": "^5.36.1",
		"babel-loader": "^8.2.5",
		"cross-env": "^7.0.3",
		"eslint": "^8.23.0",
		"eslint-config-next": "^12.2.5",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-typescript": "^3.6.0",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-mdx": "^2.0.2",
		"eslint-plugin-next": "^0.0.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"eslint-plugin-sonarjs": "^0.15.0",
		"eslint-plugin-unicorn": "^48.0.1",
		"eslint-plugin-unused-imports": "^2.0.0",
		"husky": "^8.0.1",
		"lint-staged": "^12.5.0",
		"postcss": "^8.4.16",
		"prettier": "^2.8.8",
		"typescript": "^4.8.2"
	}
}
